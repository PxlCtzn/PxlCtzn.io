{% set data = jsonLoad('data.json') %}

<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        	<link href="{{ asset('css/hexi-flexi-grid/style.css') }}" rel="stylesheet" />
        	<link href="{{ asset('fonts/fontello/css/fontello.css') }}" rel="stylesheet" />
        	<link href="{{ asset('css/theme.css') }}" rel="stylesheet" />
        {% endblock %}
    </head>
    <body>        
        <h1 id="brand">{{ app_name }}</h1>
        {% include 'partials/menu.html.twig' %}
        <div class="container">
            {% block body %}
            {% include 'partials/articles/podcast.html.twig' %}
            {% endblock %}
                </div>  
        {% include 'partials/footer.html.twig' %}
        {% block javascripts %}{% endblock %}
